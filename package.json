{
  "name": "copy-syntax-in-light",
  "displayName": "Copy Syntax In Light",
  "publisher": "AhmedAdel",
  "icon": "images/logo.png",
  "description": "",
  "version": "1.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "copy-syntax-in-light.copySyntaxInLight",
        "title": "Copy Syntax In Light Theme"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "copy-syntax-in-light.copySyntaxInLight",
          "when": "editorTextFocus",
          "group": "9_cutcopypaste@3"
        }
      ]
    },
    "keybindings": [
      {
        "command": "copy-syntax-in-light.copySyntaxInLight",
        "key": "ctrl+shift+c",
        "mac": "cmd+shift+c",
        "when": "editorTextFocus"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ahmed-Adel-Morsi/Copy-Syntax-in-Light"
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.86.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9"
  }
}
